{
    "authors": [
        "enalye"
    ],
    "copyright": "Copyright Â© 2023, Enalye",
    "description": "Moteur de jeu 2D",
    "license": "Zlib",
    "name": "atelier",
    "targetName": "atelier",
    "workingDirectory": ".",
    "dependencies": {
        "grimoire": {
            "path": "grimoire"
        },
        "farfadet": {
            "path": "farfadet"
        },
        "minuit": {
            "path": "minuit"
        },
        "bindbc-sdl": "~>1.4.2",
        "audio-formats": "~>3.0.1"
    },
    "versions": [
        "BindSDL_Static",
        "SDL_2_26",
        "SDL_Image_2_6",
        "SDL_TTF_2_20"
    ],
    "subConfigurations": {
        "bindbc-sdl": "static"
    },
    "libs-windows": [
        "$PACKAGE_DIR/libs/$PLATFORM/$ARCH/SDL2",
        "$PACKAGE_DIR/libs/$PLATFORM/$ARCH/SDL2_image",
        "$PACKAGE_DIR/libs/$PLATFORM/$ARCH/SDL2_ttf"
    ],
    "copyFiles-windows": [
        "$PACKAGE_DIR/libs/$PLATFORM/$ARCH/SDL2.dll",
        "$PACKAGE_DIR/libs/$PLATFORM/$ARCH/SDL2_image.dll",
        "$PACKAGE_DIR/libs/$PLATFORM/$ARCH/SDL2_ttf.dll"
    ],
    "lflags-posix": [
        "-L-L$PACKAGE_DIR/libs/$PLATFORM/$ARCH/",
        "-L-lz"
    ],
    "libs-posix": [
        "SDL2",
        "SDL2_image",
        "SDL2_ttf"
    ],
    "configurations": [
        {
            "name": "debug",
            "targetType": "library",
            "targetName": "atelier_debug",
            "dflags": [
                "-gf"
            ],
            "versions": [
                "AtelierDebug"
            ]
        },
        {
            "name": "etabli_debug",
            "targetType": "library",
            "targetName": "atelier_etabli_debug",
            "versions": [
                "AtelierEditor",
                "AtelierDebug"
            ]
        },
        {
            "name": "etabli",
            "targetType": "library",
            "targetName": "atelier_etabli",
            "versions": [
                "AtelierEditor"
            ]
        },
        {
            "name": "export",
            "targetType": "library",
            "targetName": "atelier",
            "targetPath": "export",
            "lflags": [
                "-subsystem:windows",
                "-entry:mainCRTStartup"
            ]
        },
        {
            "name": "etabli_export",
            "targetType": "library",
            "targetName": "atelier",
            "targetPath": "export",
            "lflags": [
                "-subsystem:windows",
                "-entry:mainCRTStartup"
            ],
            "versions": [
                "AtelierEditor"
            ]
        }
    ],
    "subPackages": [
        "./example"
    ]
}