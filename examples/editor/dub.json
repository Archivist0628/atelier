	"name": "editor",
    "targetName": "example_editor",
    "workingDirectory": "../../",
    "dependencies": {
        "atelier:game": {
            "path": "../../"
        }
    },
    "configurations": [
        {
            "name": "example_editor_debug",
            "targetType": "executable",
            "targetPath": "../bin",
            "dflags": [
                "-gf"
            ],
            "subConfigurations": {
                "atelier:game": "game_editor_debug"
            }
        },
        {
            "name": "example_editor_export",
            "targetType": "executable",
            "targetPath": "../export",
            "lflags": [
                "-subsystem:windows",
                "-entry:mainCRTStartup"
            ],
            "subConfigurations": {
                "atelier:game": "game_editor"
            }
        }
    ]
}