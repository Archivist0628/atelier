texture "editor" {
    file "editor.png"
}

sprite "editor:arrow-right" {
    texture "editor"
    clip 0 0 16 16
}

sprite "editor:arrow-down" {
    texture "editor"
    clip 16 0 16 16
}

sprite "editor:file" {
    texture "editor"
    clip 32 0 16 16
}

sprite "editor:folder-close" {
    texture "editor"
    clip 48 0 16 16
}

sprite "editor:folder-open" {
    texture "editor"
    clip 64 0 16 16
}

sprite "editor:magnify" {
    texture "editor"
    clip 80 0 16 16
}